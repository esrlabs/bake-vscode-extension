{
    "comments": {
        "lineComment": "#",
    },
    "brackets": [
        ["{", "}"]
    ],
    "autoClosingPairs": [
        { "open": "{", "close": "}" },
        { "open": "\"", "close": "\"", "notIn": ["string"] },
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["\"", "\""]
    ],
    "folding": {
        "markers": {
          "start": "^.*{\\s*$",
          "end": "^.*}\\s*$"
        }
    }
 }